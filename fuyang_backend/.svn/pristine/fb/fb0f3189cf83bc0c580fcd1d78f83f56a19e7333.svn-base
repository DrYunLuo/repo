<div id="news_manage" ng-controller="newsManageCtrl">
    <div class="table_top">
        <ul>
            <li><a href="#/newsEdit">新增</a></li>
            <li><a href="" ng-click="deleteMore()">批量删除</a></li>
            <li>
                <input type="text"/>
                <button ng-click="search()">搜索</button>
            </li>
        </ul>
    </div>
    <table class="manageTable" cellspacing="0" width="100%">
        <thead>
            <tr>
                <th><input type="checkbox" ng-click="checkAll()" ng-model="isCkeck"/></th>
                <th>新闻名称</th>
                <th>发布者</th>
                <th>分类</th>
                <th>发布时间</th>
                <th colspan="3">操作</th>
            </tr>
        </thead>
        <tbody>
            <tr ng-repeat="data in dataList track by $index">
                <td><input type="checkbox" ng-model="data.check"/></td>
                <td ng-bind="data.title"></td>
                <td ng-bind="data.author"></td>
                <td>
                    <span ng-if="data.newsType==1">通知公告</span>
                    <span ng-if="data.newsType==2">各区动态</span>
                    <span ng-if="data.newsType==3">活动信息</span>
                    <span ng-if="data.newsType==4">公益人物</span>
                </td>
                <td ng-bind="data.createdAt"></td>
                <td>推送</td>
                <td><a href="" ng-click="removeData(data)">删除</a></td>
                <td><a href="#/newsEdit/{{data.id}}">编辑</a></td>
            </tr>
        </tbody>
    </table>
    <div id="newsManagePage">

    </div>
</div>