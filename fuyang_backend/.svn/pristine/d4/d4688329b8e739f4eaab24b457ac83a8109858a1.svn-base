#org_header .selectClass{
    margin-top:10px;
}
#org_header ul li{
    float: left;
}
#org_header ul a{
    color: #000;
    font-size: 18px;
    font-weight: bold;
    padding:5px 30px;
    background: url(../img/select.png) no-repeat 0 -46px;
    cursor: pointer;
}
#org_header ul a.active{
    color: #E84C3D;
    background: url(../img/select.png) no-repeat 0 6px;
    cursor: pointer;
}
#groups_detail h4 a{
    color:#000;
    font-size: 16px;
}
#groups_detail h4 a:hover{
    color:#ee3e3c;
}
#groups_detail .media .media-left img{
    width: 200px;
    height:140px;
}
.groups_title span{
    height: 100%;
    padding:8px 15px;
    background-color: #e84c3d;
    color: #fff;
    text-align: center;
    font-size: 1.4rem;
}
.bread{
    background-color:#dcdcdc;
}
.bread li{
    float: left;
    padding:8px 0;
}
.bread li a.g_clc{
    float: left;
    padding:0 15px;
    font-size: 1.4rem;
    border-right: 1px solid #444444;
}
/*.bread li a.g_clc:hover{*/
/*color: #e84c3d;*/
/*}*/
.bread li a.groups_active{
    color:#e84c3d;
    text-decoration: none;
}
.bread li:last-child a{
    border:0;
}
/*************groups_details**************/
.media-heading{
    font-size: 1.3rem;
    font-weight: 600;
    padding:5px 0;
}
/**************pagination****************/
.pagination{
    padding:0 24%;
}
.pagination li a{
    color: #444;
}
#groups_main .pagination li a:hover{
    color: #fff;
    background-color: #e84c3d;
}

/***************dropdown*******************/
.dropdown{
    padding:5%;
}
.group_selec{
    height:30px;
    outline: 0;
    text-align: center;
    background-color:#eee;
    border: 1px solid #9d9c9c;
}
.dropdown button{
    background-color:#eee;
    height:30px;
    border:1px solid #e84c3d;
}
.search{
    padding:5%;
    position: relative;
}
.search span{
    display: inline-block;
    width:19px;
    height:16px;
    background: url("../img/01.png")no-repeat center center;
    position: absolute;
    top: 34%;
    right: 11%;
    cursor: pointer;
}
.search input{
    border: 1px solid #9d9c9c;
    background-color:#eee;
    height:30px;
    padding-left: 10px;
}
.search_groups{
    padding:0;
}
.create_group{
    padding:9% 2%;
    position: relative;
    border-top: 0;
}
.create_group button{
    display:block ;
    margin: 0 auto;
    padding:5% 10% 5% 25%;
    background-color: #e84c3d;
    border:0;
    border-radius:5px;
    color: #ffffff;
    font-size: 1.4rem;
}
.create_group span{
    display: inline-block;
    background: url("../img/03.png")no-repeat center center;
    width:24px;
    height:24px;
    position: absolute;
    top:37%;
    left:28%;
}

.group_news{
    margin-top:5px;
    background-color:#eee;
    padding:10px 0 10px 0;
    border:2px solid #dadada;

}
.group_news h3{
    background-color:#e84c3d;
    border-top-left-radius:10px;
    padding:3% 5% ;
    text-align: center;
    color: #fff;
    margin-left:10px;
    font-size: 1.2rem;
}
.group_news_content{
    border-top:2px solid #dadada;
    line-height:100%;
    padding-top: 10px;
}
.group_news_content li{
    list-style-type: disc;
    line-height:20px;
}
.group_news_content li a{
    display: inline-block;
    width:95%;
    vertical-align: middle;
    line-height:20px;
    white-space:nowrap;
    -ms-text-overflow: ellipsis;
    text-overflow: ellipsis;
    overflow: hidden;
}
#myfooter .row #context{
    margin-top:20px;
}
#groupPic img{
    width:100%;
}
#right_container .content_box .title h3{
    padding:10px 0 10px 60px;
    font-size: 22px;
    margin: 0;
    background: url(../img/hot.png) no-repeat 15px 0;
}
#right_container .content_box .title h5{
    margin:10px 0;
}
#right_container .content_box ul li:before{
    content: "";
    display: inline-block;
    width: 5px;
    height: 5px;
    margin: 0 4px 7px;
    background-color: #e56752;
    border-radius: 50%;
}
#right_container .content_box ul li a{
    display: inline-block;
    width: 90%;
    text-overflow:ellipsis;
    white-space:nowrap;
    overflow:hidden;
    color: #000;
}
#right_container .content_box ul li p{
    font-size: 12px;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 3;
    -webkit-box-orient: vertical;
}
#left_container .media .col{
    padding-right:15px;
}
.groups_description{
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 3;
    -webkit-box-orient: vertical;
}
@media screen and (max-width: 768px) {
    #groups_detail .col-xs-6 {
        width: 100%;
        margin-top: 0;
    }
    /******************左边******************/

    .bread li{
        padding:8px 0;
    }
    .bread li a.g_clc{
        padding:0 8px;
    }
    .media-heading{
        padding:0 0;
    }
    .pagination{
        padding:0 13%;
    }
    .group_news_content li{
        line-height:30px;
    }
    #groups_detail .media .media-left img {
        width: 150px;
        height: 120px;
    }
    .person {
        padding:0;
    }
    .groups_mess {
        padding:0;
    }
    .groups_description{
        display: none;
    }
    /******************右边******************/
    .search_groups{
        padding:0 1%;
    }
    .create_group button{
        font-size: 1.6rem;
        padding:2% 10% 2% 25%;
    }
    .create_group span{
        position: absolute;
        top:38%;
        left:33%;
    }
    #left_container div{
        font-size: 12px;
    }
}
@media screen and (min-width: 769px) and (max-width:991px){

    /******************左边******************/
    .groups_title span{
        padding:8px 10px;
    }
    .bread li{
        padding:8px 0;
    }
    .bread li a.g_clc{
        padding:0 7px;
    }
    .pagination{
        padding:0 10%;
    }
    .person{
        padding:2px 0;
    }
    .groups_mess{
        padding:2px 0;
    }
    .media-heading{
        margin:0;
        padding:4px 0;
    }
    .group_news_content li{
        line-height:25px;
    }
    /******************右边******************/
    .dropdown button{
        padding:0;
    }
    .create_group span{
        position: absolute;
        top:34%;
        left:25%;
    }
}
@media screen and (min-width:992px) and (max-width:1199px){

    /*****************左边******************/
    .bread li{
        padding:8px 0;
    }
    .bread li a.g_clc{
        padding:0 14px;
    }
    .media-heading{
        margin:4px;
        padding:8px 0;
    }
    .pagination{
        padding:0 15%;
    }
    .group_news_content li{
        line-height:25px;
    }
    /******************右边******************/
    .search span{
        top: 37%;
        right: 15%;
    }
    .create_group span{
        position: absolute;
        top:38%;
        left:30%;
    }
}
@media screen and (min-width:1200px) {

    /******************左边******************/
    .pagination{
        padding:0 20%;
    }
    .group_news_content li{
        line-height:23px;
    }
    /******************右边******************/
    .create_group span{
        position: absolute;
        top:38%;
        left:32%;
    }
}

















































