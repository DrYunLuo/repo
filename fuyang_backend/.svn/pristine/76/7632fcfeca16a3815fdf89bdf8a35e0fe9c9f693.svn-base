*{
    marin:0px;
    padding:0px;
}
a{
    text-decoration:none;
}
li{
    list-style:none;
}
/*头部部分。。。*/
header{
    width:100%;
    height:4rem;
    background:#e94c3d;
    position:relative;
}
header h1{
    margin-top:0px;
    text-align:center;
    font-size:2.5rem;
    color:#fffdf9;
    line-height:4rem;
}
header .mylink1,header .mylink2{
    display:block;
    position:absolute;
}
header .mylink1{
    bottom:10%;
    left:3%;
}
header .mylink2{
    bottom:10%;
    right:3%;
}
/*article正文内容部分。。。*/

.myh1{
    width:100%;
    text-align:center;
    font-size:1.8rem;
    font-weight:bold;
    height:4rem;
    line-height:4rem;
}
article{
    width:90%;
    margin-left:5%;
    margin-right:5%;
    height:7rem;
}
article section{
    width:100%;
    position:relative;
}
article section .myaside1,article section .myaside2,article section .myaside3{
    position:absolute;
}
article section .myaside1{
    top:0;
    left:0;
    width:33%;
}
article section .myaside1 div,article section .myaside2 div{
    width:100%;
    height:1rem;
    background:#e84c40;
}
article section .myaside3 div{
    width:100%;
    height:1rem;
    background:#cdcdcd;
}
article section aside p{
    width:100%;
    height:3rem;
    font-size:1.8rem;
    /*line-height:50px;*/
    text-align:center;
    font-weight:bold;
}
article section .myaside2{
    top:0;
    width:33%;
    left:33%;
}
article section .myaside3{
    width:34%;
    top:0;
    left:66%;
}
.myhr1{
    border-top:1px solid #d8dcdb;
    border-bottom:1px solid #d8dcdb;
    width:100%;
    height:1rem;
    margin-top:6rem;
    background-color:#f1f5f4;
}
.myp1{
    width:90%;
    padding-top:0.8rem;
    padding-bottom:0.8rem;
    padding-left:5%;
    padding-right:5%;
    font-size:1.8rem;
    line-height:3rem;
    font-weight:bold;
    color:#3d3d3d;
}
.object{
    width:90%;
    margin-right:5%;
    margin-left:5%;
    margin-top:1rem;
}
.object p{
    margin:0px!important;
    width:100%;
    height:3rem;
    border-top:1px dashed #acacac;
    line-height:3rem;
    font-size:1.5rem;
    color:#5e5e5e;
}
.object p:last-child{
    border-bottom:1px dashed #acacac;
}
.object p span:nth-child(1){
	width:40%;
	height:100%;
	font-size:1.2rem;
	color:#262626;
	display:block;
	float:left;
	font-size:1.5rem;
}
.object p span:nth-child(2){
	width:50%;
	height:100%;
	font-size:1rem;
	color:#262626;
	display:block;
	float:left;
	font-size:1.5rem;
}

.myp2{
    margin-left:5%;
    margin-right:5%;
    width:90%;
    font-size:1.2rem;
    line-height:3rem;
    color:#929292;
}
.myp2 span{
    color:#f71226;
    font-size:1.5rem;
}
button{
    width:100%;
    display:block;
    margin-top:4rem;
    text-align:center;
    line-height:4rem;
    height:4rem;
    color:#fef2f6;
    background-color:#e94c3d;
    font-size:2.5rem;
}