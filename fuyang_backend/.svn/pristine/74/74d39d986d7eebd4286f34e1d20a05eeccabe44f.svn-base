/********hidden3*********/
.testify{
    background-color: #fff;
    border:1px solid #d0d0d0;
    padding-bottom: 25px;
}
.testify:after{
    content: '';
    display: block;
    clear: both;
}
.testify h2{
    padding:15px 0 15px 20px;
    font-size: 1.7rem;
    color: #000;
    border-bottom: 1px solid #d0d0d0;
    background-color: #f5f5f5;
}
.testify h3{
    padding:40px 125px;
    font-size: 1.6rem;
    text-align: center;
}

.testify div{
    width: 49.5%;
    display: inline-block;
    position: relative;
    text-align: center;
}
.testify form input[type=file]{
    width:200px;
    height: 200px;
    opacity:0;
    z-index:100;
    border:1px solid #9a9a9a;
    position:absolute;
    left:calc(50% - 100px);
    top:0;
}
.testify .touch{
	display:block;
    width:43%;
    padding:5px 0;
    font-size: 1.6rem;
    text-align: center;
    margin-left:calc(50% - 21.5%);
    background-color:#e4393c;
    border:0;
    border-radius:10px;
    color:#fff;
}
.testify .image_container{
    width:200px;
    height:200px;
    /*background-color: #1b6d85;*/
    position: relative;
}
.image_container span{
    display: inline-block;
    width:200px;
    height:200px;
    line-height:200px;
    text-align: center;
    font-size: 1.6rem;
    border:1px solid #9a9a9a;

    z-index: 5;
}
.testify .image_container img{
    width:100%;
    height:100%;
    z-index: 10;
    position: absolute;
    left:0;
    top:0;
}
/************complete************/
.complete{
    margin-top: 60px;
    background-color: #fff;
    height:396px;
    line-height:396px;
}
