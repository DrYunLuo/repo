*{
    margin:0;
    padding: 0;
    font-family: "Helvetica Neue", Helvetica, Arial, sans-serif;
    font-size: 12px;
    color: #444;
    box-sizing: border-box;
}
a{
    color: #444;
}
a:hover{
    text-decoration: none;
    color: #ee3e3c;
}
body{
    background-color: #f5f5f5;
}
li{
    list-style: none;
}
h1,h2,h3,h4,p,img,div,ul,li,ol,hr{
    font-size: 1rem;
    margin: 0;
    padding:0;
}
.container{
    width:80%;
    padding: 0;
}
.col-xs-12,.col-sm-12,.col-md-12,.col-lg-12,.col-xs-11,.col-sm-11,.col-md-11,.col-lg-11,.col-xs-10,.col-sm-10,.col-md-10,.col-lg-10,.col-xs-9,.col-sm-9,.col-md-9,.col-lg-9,.col-xs-8,.col-sm-8,.col-md-8,.col-lg-8,.col-xs-7,.col-sm-7,.col-md-7,.col-lg-7,.col-xs-6,.col-sm-6,.col-md-6,.col-lg-6,.col-xs-5,.col-sm-5,.col-md-5,.col-lg-5,.col-xs-4,.col-sm-4,.col-md-4,.col-lg-4,.col-xs-3,.col-sm-3,.col-md-3,.col-lg-3,.col-xs-2,.col-sm-2,.col-md-2,.col-lg-2,.col-xs-1,.col-sm-1,.col-md-1,.col-lg-1,.form-control{
    padding:0;
    margin:0;
}
/***************header****************/
#header{
    margin-top: 20px;
}
/****************nav******************/
#nav:after{
    content: '';
    display: block;
    clear: both;
}
.gray{
    height:60px;
    background-color: #d0d0d0;
    position: relative;
}
.gray .simple{
    height:100%;
    background-color: #d0d0d0;
}
.gray .simple h2{
    font-size: 2rem;
    height:100%;
    line-height:60px;
    text-align: center;
}

.gray .second{
    position: absolute;
    left:21.5%;
    top:0;
    z-index: 10;
    width:60px;
    height:60px;
    line-height:60px;
    text-align: center;
    background-color: #fff;
    border-radius:50%;
}

.gray .active{
    background-color:#e84c3d;
}
.gray .active h2{
    color: #fff;
}
.gray .forth{
    position: absolute;
    left:46.5%;
    top:0;
    z-index: 10;
    width:60px;
    height:60px;
    line-height:60px;
    text-align: center;
    background-color: #fff;
    border-radius:50%;
}
.gray .sixth{
     position: absolute;
    left:71.5%;
    top:0;
    z-index: 10;
    width:60px;
    height:60px;
    line-height:60px;
    text-align: center;
    background-color: #fff;
    border-radius:50%;
 }
@media screen and (min-width: 768px) {
    .gray .second{
        left:21%;
    }
    .gray .forth{
        left:46.5%;
    }
    .gray .sixth{
        left:71.5%;
    }
}
@media screen and (min-width: 1200px) {
    .gray .second{
        left:22.5%;
    }
    .gray .forth{
        left:47.5%;
    }
    .gray .sixth{
        left:72.5%;
    }
}
/******************main********************/
#main:before{
    content: '';
    display: table;
    clear: both;
}
#main:after{
    content: '';
    display: block;
    clear: both;
}
/********hidden***********/
#hidden1{
    margin-top: 20px;
}
#hidden1:after{
    content:'';
    display: block;
    clear: both;
}
