package com.yichuang.fuyang.entity;

import java.io.Serializable;
import java.util.Date;

public class Postbbsreplay implements Serializable{

	/**
	 * 论坛帖子回复信息
	 */
	private static final long serialVersionUID = 6144588457439635635L;
	
	private String id;
	private String content;
	private String volunteerId;
	private String postatomsbbsId;
	private Date replayTime;
	private Volunteers volunteers;
	public String getId() {
		return id;
	}
	public void setId(String id) {
		this.id = id;
	}
	
	public String getContent() {
		return content;
	}
	public void setContent(String content) {
		this.content = content;
	}
	public String getVolunteerId() {
		return volunteerId;
	}
	public void setVolunteerId(String volunteerId) {
		this.volunteerId = volunteerId;
	}
	public String getPostatomsbbsId() {
		return postatomsbbsId;
	}
	public void setPostatomsbbsId(String postatomsbbsId) {
		this.postatomsbbsId = postatomsbbsId;
	}
	public Date getReplayTime() {
		return replayTime;
	}
	public void setReplayTime(Date replayTime) {
		this.replayTime = replayTime;
	}
	public Volunteers getVolunteers() {
		return volunteers;
	}
	public void setVolunteers(Volunteers volunteers) {
		this.volunteers = volunteers;
	}
	
	
}
