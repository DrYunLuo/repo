<!--精品活动-->
<div id="fineAct" ng-controller="fineActCtrl">
    <table class="manageTable" cellspacing="0" width="100%">
<div class="container" style="margin-top: 0.5em;">
        <input type="text" id="select_txt" style="border-radius:5px;height: 30px;width: 200px;"><button class="btn btn-danger btn-sm" style="margin-bottom: 3px;" ng-click="fineselect()">搜索</button>
</div>
        <thead>
        <tr>
            <th><input type="checkbox" ng-click="checkAll()" ng-model="isCkeck"/></th>
            <th>活动名称</th>
            <th>发布时间</th>
            <th>类型</th>
            <th colspan="3">操作</th>
        </tr>
        </thead>
        <hr/>
        <tbody>
        <tr ng-repeat="data in fineAct track by $index">
            <td><input type="checkbox" ng-bind="data.id" ng-checked="all" ng-click="finebind()" /></td>
            <td ng-bind="data.title"> </td>
            <td ng-bind="data.createdAt"></td>
            <td ng-bind="data.hostUnit"></td>
            <td ng-if="data.status==2">待审核</td>
            <td ng-if="data.status==1" style="color: red;" ng-click="fine_push()"><a href="">推送</a></td>
            <td ng-if="data.status==0" >>未通过/修改</td>
        </tr>

        </tbody>
    </table>
    <table class="manageTable" cellspacing="0" width="100%" >
    <tbody>
    <tr>
        <td><input type="checkbox" ng-model="all" style="margin-top: 10px;" value="批量选择" />批量选择</td>
        <td > <button class="btn btn-danger btn-sm" ng-click="fine_push()">推送</button></td>
        <td ></td>
        <td></td>
        <td ></td>
        <td></td>
        <td> <div id="fineManagePage">

        </div>
        </td>
    </tr>
    </tbody>
    </table>
</div>
